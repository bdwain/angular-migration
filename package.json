{
  "name": "react-skeleton",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config ./config/dev.webpack.config.js --progress --colors --hot --inline --port 5100 --history-api-fallback",
    "prebuild": "yarn clear && mkdir dist",
    "build": "webpack --config ./config/prod.webpack.config.js",
    "build-snapshot": "yarn build && mv dist/app.js dist/app@$(git rev-parse HEAD).js && mv dist/app.css dist/app@$(git rev-parse HEAD).css && replace 'app' \"app@$(git rev-parse HEAD)\" -- dist/index.html",
    "build-release": "yarn build && mv dist/app.js dist/app@$npm_package_version.js && mv dist/app.css dist/app@$npm_package_version.css && replace 'app' \"app@$npm_package_version\" -- dist/index.html",
    "clear": "rm -rf dist/",
    "prod-test": "yarn build && node prod-test-server.js --port 5101",
    "eslint": "eslint ./src -c ./config/.eslintrc.json",
    "test": "karma start ./config/test/karma.conf.js --single-run --browsers PhantomJS",
    "verify": "yarn eslint && yarn test",
    "test-debug": "sambac --webpackConfig ./config/test/tests.individual.webpack.js --port 5102 --webpackPort 5103"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@stash.grubhub.com:7999/spike/react-skeleton.git"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.5.1",
    "eslint": "^3.9.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.6.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.6.5",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "jasmine-expect-jsx": "^1.1.2",
    "jasmine-reporters": "^2.1.1",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^0.3.8",
    "karma-jasmine-expect-jsx": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "morgan": "^1.7.0",
    "node-sass": "^3.4.2",
    "node-sass-globbing": "^0.0.23",
    "phantomjs-prebuilt": "^2.1.13",
    "postcss": "^5.2.5",
    "postcss-loader": "^1.1.1",
    "react-addons-test-utils": "^15.0.1",
    "sambac-webpack": "^0.3.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-jasmine-html-runner-plugin": "^0.5.0",
    "yargs": "^6.4.0"
  }
}
